<?xml version="1.0" encoding="utf-8"?>
  <odoo>
      
    <!-- View stock.picking form -->
    <record id="view_stock_picking_form_location_analytic_distribution" model="ir.ui.view">
        <field name="name">view.stock.picking.form.location.analytic.distribution</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock_analytic.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='move_ids_without_package']/tree/field[@name='analytic_distribution']" position="attributes">
              <attribute name="widget">location_analytic_distribution</attribute>
            </xpath>
        </field>
    </record>

  </odoo>