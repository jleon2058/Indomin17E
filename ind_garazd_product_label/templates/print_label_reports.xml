<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="paperformat_label_80x30" model="report.paperformat">
        <field name="name">Etiquetas 80x30 mm</field>
        <field name="format">custom</field>
        <field name="page_height">30</field>
        <field name="page_width">80</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="dpi">80</field>
        <field name="default" eval="False"/>
    </record>

    <record id="action_report_product_label_transfers" model="ir.actions.report">
        <field name="name">Etiquetas Transferencias (80x30mm)</field>
        <field name="model">print.product.label.line</field>
        <field name="report_type">qweb-pdf</field>
        <field name="paperformat_id" ref="paperformat_label_80x30"/>
        <field name="report_name">ind_garazd_product_label.report_stock_transfer_label_template</field>
        <field name="report_file">ind_garazd_product_label.report_stock_transfer_label_template</field>
        <field name="print_report_name">'Etiquetas para productos desde transferencias'</field>
    </record>

    <record id="action_report_product_label_products" model="ir.actions.report">
        <field name="name">Etiquetas Productos (80x30mm)</field>
        <field name="model">print.product.label.line</field>
        <field name="report_type">qweb-pdf</field>
        <field name="paperformat_id" ref="paperformat_label_80x30"/>
        <field name="report_name">ind_garazd_product_label.report_products_80x30_template</field>
        <field name="report_file">ind_garazd_product_label.report_products_80x30_template</field>
        <field name="print_report_name">'Etiquetas para productos'</field>
    </record>

</odoo>
