<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_product_product_inherited" model="ir.ui.view">
            <field name="name">product.product.view.form.inherit.ind_parent_product</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page name="parent_products" string="Productos Originales">
                        <field name="productos_padre" />
                    </page>
                </xpath>
            </field>
        </record>

</odoo>
