<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="IndHrChecklist.ProgressBar" owl="1">
        <div class="progress">
            <div class="progress-bar" t-att-class="progressBarClass" role="progressbar" t-att-style="{'width': props.value + '%'}" t-att-aria-valuenow="props.value" aria-valuemin="0" aria-valuemax="100">
                <t t-esc="props.value"/>%
            </div>
        </div>
    </t>
</templates>