<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <record id="report_ingreso_xlsx" model="ir.actions.report">
        <field name="name">Reporte de Ingresos</field>
        <field name="model">stock.picking</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">ind_stock.reporte_ingresos</field>
        <field name="report_file">ind_stock.reporte_ingresos</field>
        <field name="binding_model_id" ref="ind_stock.model_stock_picking"/>
        <field name="binding_type">report</field>
        <field name="attachment_use" eval="False"/>
    </record>

    <record id="report_salidas_xlsx" model="ir.actions.report">
        <field name="name">Reporte de Salidas</field>
        <field name="model">stock.picking</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">ind_stock.reporte_salidas</field>
        <field name="report_file">ind_stock.reporte_salidas</field>
        <field name="binding_model_id" ref="ind_stock.model_stock_picking"/>
        <field name="binding_type">report</field>
        <field name="attachment_use" eval="False"/>
    </record>
    
</odoo>